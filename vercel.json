{
    "version": 2,
    "name": "backend",
    "builds": [
      { "src": "index.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/index.js", "methods": ["GET", "POST"] }
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "functions": { "backend/index.js": { "runtime": "nodejs16" } },
    "output": {}
  }
  